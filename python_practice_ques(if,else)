   "source": [
    "def fact(n):\n",
    "    if n<=1:\n",
    "        return 1\n",
    "    else:\n",
    "        return fact(n-1)*n\n",
    "result_fact=fact(5)\n",
    "print(\"Factorial of 5 is\",result_fact)"
   ]
