    "def print_sum(n):\n",
    "    if n==0:\n",
    "        return 0\n",
    "    else:\n",
    "        return print_sum(n-1)+n\n",
    "result_sum=print_sum(10)\n",
    "print(\"Sum of {n} is {result_sum}\".format(n=10,result_sum=result_sum))"
   ]
