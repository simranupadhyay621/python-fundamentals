     ]
    }
   ],
   "source": [
    "#Alternate\n",
    "def count_char_frequency(s):\n",
    "    freq = {}\n",
    "    for char in s:\n",
    "        freq[char] = freq.get(char, 0) + 1\n",
    "    return freq\n",
    "# Example usage\n",
    "input_string = \"hello world\"\n",
    "frequency = count_char_frequency(input_string)\n",
    "print(frequency)"
   ]
