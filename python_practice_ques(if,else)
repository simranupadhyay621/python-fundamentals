    "#Alternate\n",
    "def rep_poor(str):\n",
    "    if \"not\" in str and \"poor\" in str:\n",
    "        start=str.find(\"not\")\n",
    "        end=str.find(\"poor\")\n",
    "        if start<end:\n",
    "            return str[:start]+\"good\"+str[end+4:]\n",
    "    return str\n",
    "print(rep_poor(\"The lyrics is not that poor. not that poor\"))\n",
    "print(rep_poor(\"The poor service was not expected\"))\n",
