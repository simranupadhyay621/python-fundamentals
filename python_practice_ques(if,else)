    "def not_poor(txt):\n",
    "    idx_1=txt.find(\"not\")\n",
    "    idx_2=txt.find(\"poor\")\n",
    "    if idx_2>idx_1 and idx_1>0 and idx_2>0:\n",
    "        txt=txt.replace(txt[idx_1:(idx_2+4)],\"good\")\n",
    "        return txt\n",
    "    else:\n",
    "        return txt\n",
    "print(not_poor(\"The lyrics is not that poor. not that poor\"))\n",
    "print(not_poor(\"The lyrics is poor\"))"
   ]
