     ]
    }
   ],
   "source": [
    "#Combine all dictionary into one\n",
    "dic1={1:10, 2:20}\n",
    "dic2={3:30, 4:40}\n",
    "dic3={5:50,6:60}\n",
    "dic={**dic1,**dic2,**dic3}\n",
    "print(dic)\n",
    "\n",
    "#Alternate\n",
    "dic_new={}\n",
    "dic_new.update(dic1)\n",
    "dic_new.update(dic2)\n",
    "dic_new.update(dic3)\n",
    "print(dic_new)"
   ]
  },
